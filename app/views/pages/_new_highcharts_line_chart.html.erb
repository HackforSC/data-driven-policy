<script type="text/javascript">
  $(function(){
    new Highcharts.Chart({         
      chart: {
        renderTo: "line_chart",
        type: 'area'
      },
      title: {
        text: <% @locations.count.times do %>
      },
      subtitle: {
        text: "Title"
      },
      xAxis: {        
        type: 'datetime',
        dateTimeLabelFormats: {
          month: '%e. %b',
          year: '%b'
        }
      },
      yAxis: {
        title: {
          text: "Axis Label"
        },
        min: 0
      },
      tooltip:{
        formatter: function(){
          return Highcharts.dateFormat("%B %e %Y", this.x) + ':' + Highcharts.numberFormat(this.y, 1);
        }
      },
      series: [      
        {
          name: 'Working',
          data:  [ [ 102 * 1000, 103 * 1000] ]
        },
        {
          name: 'Failing',
          data: "#{raw [ [ 102 * 1000, 103 * 1000] ]}"
        }
      ]
    });
  });
</script>